# Firebase Configuration
FIREBASE_API_KEY=your-firebase-api-key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789012
FIREBASE_APP_ID=your-app-id
FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# AI Provider API Keys
OPENROUTER_API_KEY=sk-or-v1-your-openrouter-key
OPENAI_API_KEY=sk-your-openai-key
GOOGLE_API_KEY=AIza-your-google-key
GROQ_API_KEY=gsk_your-groq-key

# API Endpoints (optional - defaults are used if not set)
OPENROUTER_API_URL=https://openrouter.ai/api/v1
OPENAI_API_URL=https://api.openai.com/v1
GOOGLE_API_URL=https://generativelanguage.googleapis.com/v1beta
GROQ_API_URL=https://api.groq.com/openai/v1
OLLAMA_API_URL=http://localhost:11434

# AI Model Configuration
OPENROUTER_DEFAULT_MODEL=google/gemini-2.0-flash-exp
OPENROUTER_MAX_TOKENS=4000
OPENROUTER_TEMPERATURE=0.7

OPENAI_DEFAULT_MODEL=gpt-4
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.7

GOOGLE_DEFAULT_MODEL=gemini-2.0-flash-exp
GOOGLE_MAX_TOKENS=4000
GOOGLE_TEMPERATURE=0.7

GROQ_DEFAULT_MODEL=llama-3.1-70b-versatile
GROQ_MAX_TOKENS=4000
GROQ_TEMPERATURE=0.7

OLLAMA_DEFAULT_MODEL=llama3.2
OLLAMA_MAX_TOKENS=4000
OLLAMA_TEMPERATURE=0.7

# Application Settings
APP_NAME=n8n Automation Sidekick
APP_VERSION=1.0.0
NODE_ENV=development
DEBUG=false

# Workflow Settings
WORKFLOW_MAX_NODES=50
WORKFLOW_MAX_CONNECTIONS=100
WORKFLOW_AUTO_SAVE=true
WORKFLOW_EXPORT_FORMAT=n8n

# UI Settings
UI_THEME=light
UI_LANGUAGE=en
UI_ANIMATIONS=true
